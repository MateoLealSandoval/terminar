+    tail -20 /tmp/auth-ms.log
+else
+    echo "‚úÖ Login exitoso!"
+fi
+
+# 7. Limpiar archivos temporales
+rm -f /tmp/auth-ms.log /tmp/gateway.log
+
+echo -e "\n5Ô∏è‚É£ Verificando comunicaci√≥n NATS..."
+curl -s http://localhost:8222/connz | python3 -c "
